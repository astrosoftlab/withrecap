{
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "files.associations": {
    ".parcelrc": "json"
  },
  "files.eol": "\n",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.workingDirectories": ["packages/app"],
  "FSharp.analyzersPath": ["packages/Analyzers", "analyzers"]
}
